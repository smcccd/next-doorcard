{
  "name": "next-doorcard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "node scripts/setup-env.js",
    "dev": "npm run setup && node scripts/dev-safe.js",
    "dev:unsafe": "npm run setup && next dev",
    "dev:check": "curl -s http://localhost:3000 > /dev/null && echo 'Server running at http://localhost:3000' || echo 'No server running'",
    "build": "npm run setup && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "npm run setup && prisma generate",
    "db:push": "npm run setup && prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "build:production": "npm run db:generate && npm run build",
    "type-check": "tsc --noEmit",
    "test": "npx vitest run",
    "test:watch": "npx vitest",
    "test:coverage": "npx vitest run --coverage",
    "test:db:setup": "prisma db push --force-reset --accept-data-loss",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "test:e2e:open": "start-server-and-test dev http://localhost:3000 cypress:open",
    "test:e2e:smart": "node scripts/cypress-runner.js",
    "cypress:verify": "cypress verify && cypress info",
    "import-legacy": "tsx scripts/import-legacy.ts",
    "import-legacy:dry": "tsx scripts/import-legacy.ts --dry",
    "import-legacy:debug": "tsx scripts/import-legacy-debug.ts",
    "import-legacy:debug-dry": "tsx scripts/import-legacy-debug.ts --dry",
    "import-legacy:rollback": "tsx scripts/rollback-import.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:test": "test-storybook",
    "storybook:test:ci": "test-storybook --ci",
    "vpat:generate": "node scripts/generate-vpat.js",
    "vpat:verify": "npm run vpat:generate && echo 'VPAT generated successfully'",
    "accessibility:test": "npm run cypress:run -- --spec='cypress/e2e/accessibility.cy.ts'",
    "accessibility:report": "npm run accessibility:test && npm run vpat:generate",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@microsoft/clarity": "^1.0.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@react-pdf/renderer": "^4.3.0",
    "@sentry/nextjs": "^9.42.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/dompurify": "^3.0.5",
    "@types/marked": "^5.0.2",
    "@vercel/analytics": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parser": "^3.2.0",
    "dompurify": "^3.2.6",
    "highlight.js": "^11.11.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "lucide-react": "^0.475.0",
    "marked": "^16.1.1",
    "moment": "^2.30.1",
    "next": "^15.4.4",
    "next-auth": "^4.24.11",
    "react": "^19.0.0",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^19.0.0",
    "recharts": "^3.1.0",
    "tailwind-merge": "^3.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.9.0",
    "@next/bundle-analyzer": "^15.4.6",
    "@storybook/addon-a11y": "^9.0.18",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-onboarding": "^9.0.18",
    "@storybook/nextjs": "^9.0.18",
    "@storybook/test-runner": "^0.23.0",
    "@svgr/cli": "^8.1.0",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cypress": "^0.1.6",
    "@types/cypress-axe": "^0.8.0",
    "@types/jsdom": "^21.1.7",
    "@types/jspdf": "^1.3.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "axe-playwright": "^2.1.0",
    "commander": "^14.0.0",
    "cypress": "^14.5.2",
    "cypress-axe": "^1.6.0",
    "cypress-real-events": "^1.14.0",
    "eslint": "^9",
    "eslint-config-next": "^15.4.4",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-storybook": "^9.0.18",
    "fast-csv": "^5.0.2",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "node-fetch": "^3.3.2",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "prisma": "^6.13.0",
    "start-server-and-test": "^2.0.12",
    "storybook": "^9.0.18",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,yml,yaml}": "prettier --write"
  },
  "volta": {
    "node": "22.17.1"
  }
}
